<?xml version="1.0" encoding="UTF-8"?>
<test-set xmlns:xs="http://www.w3.org/2001/XMLSchema"
          xmlns="http://www.w3.org/2012/10/xslt-test-catalog"
          name="package">
   <description>Tests xsl:package in XSLT 3.0</description>

   <dependencies>
      <spec value="XSLT30+"/>
   </dependencies>

   <test-case name="package-901">
      <description>Error - package has no name</description>
      <created by="Michael Kay" on="2013-07-08"/>
      <test>
         <package file="package-901.xsl" role="primary"/>
         <initial-template name="main"/>
      </test>
      <result>
         <error code="XTSE0010"/>
      </result>
   </test-case>
   

</test-set>
